	driver.findElement(By.xpath("//button[contains(.,'Manage Payment Defaults')]")).click();
	driver.manage().timeouts().implicitlyWait(15, TimeUnit.SECONDS);
	driver.findElement(By.xpath("//label[contains(text(),'Manage " + G + " Defaults')]")).click(); 
	driver.manage().timeouts().implicitlyWait(15, TimeUnit.SECONDS);
	driver.findElement(By.xpath("//button[contains(.,'Select')]")).click();
	driver.manage().timeouts().implicitlyWait(15, TimeUnit.SECONDS);
	driver.findElement(By.xpath("/html/body/div[7]/div[2]/div/div[2]/div/div[2]/div[2]/div/div/div/form/div[2]/div/div/div/div/div/div/div[2]/div[3]/div/div/div/div/div/div/div/div/div/div[3]/div/div/div[2]/table/thead/tr/th[6]/a")).click();
	Thread.sleep(1500);
	driver.findElement(By.xpath("/html/body/div[7]/div[2]/div/div[2]/div/div[2]/div[2]/div/div/div/form/div[2]/div/div/div/div/div/div/div[2]/div[3]/div/div/div/div/div/div/div/div/div/div[3]/div/div/div[2]/table/tbody[1]/tr[1]/td[1]/div/input")).click(); 
	driver.manage().timeouts().implicitlyWait(15, TimeUnit.SECONDS);
	driver.findElement(By.xpath("//button[contains(.,'Complete Default Payment Recovery')]")).click();
	}
