   @Test (priority = 25) 
   public void manageAttachmentOne () throws Exception{
	   driver.findElement(By.partialLinkText("Manage PR Details")).click();
	   driver.manage().timeouts().implicitlyWait(5, TimeUnit.SECONDS); // wait 2 secs 
	   driver.findElement(By.xpath("//label[contains(.,'Manage Attachments')]")).click();
	   driver.manage().timeouts().implicitlyWait(5, TimeUnit.SECONDS); // wait 2 secs 
	   driver.findElement(By.xpath("//button[contains(.,'Continue')]")).click();
	   driver.manage().timeouts().implicitlyWait(5, TimeUnit.SECONDS); // wait 2 secs 
	   driver.findElement(By.xpath("//button[contains(.,'Add Attachment')]")).click();
	   driver.manage().timeouts().implicitlyWait(5, TimeUnit.SECONDS); // wait 2 secs 
	   driver.findElement(By.xpath("//input[@type='file']")).click();
	   Runtime.getRuntime().exec("C:\\Users\\edwardsm\\Desktop\\AutoIT\\Fileupload.exe");
	   Thread.sleep(3000);
	   driver.findElement(By.className("gwt-ListBox")).sendKeys("Supporting Documentation");
	   Thread.sleep(1000);
	   driver.findElement(By.xpath("//label[contains(.,'Yes, includes controlled unclassified information')]")).click();
	   driver.findElement(By.xpath("//label[contains(.,'Yes, include in PR Package')]")).click();
	   driver.findElement(By.xpath("//button[contains(.,'Add Attachment')]")).click();} 
   
